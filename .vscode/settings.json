{
	"cSpell.words": ["Baseapp", "enctype", "endfor", "homeppage", "REFERER", "stylesheet"],
	"workbench.colorTheme": "Dracula Theme",
	"python.defaultInterpreterPath": ".venv/Scripts/python.exe",
	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				"scope": "entity.name.function.python",
				"settings": {
					"foreground": "#9ffb80"
				}
			},
			{
				"scope": "text.html.django",
				"settings": {
					"foreground": "#e4e4e4"
				}
			},
			{
				"scope": "punctuation.definition.tag.begin.html, punctuation.definition.tag.end.html",
				"settings": {
					"foreground": "#8080ff"
				}
			},
			{
				"scope": "meta.attribute.python",
				"settings": {
					"foreground": "#eb87f4"
				}
			},
			{
				"scope": "meta.function-call.generic.python",
				"settings": {
					"foreground": "#00fd99"
				}
			},
			{
				"scope": "keyword.control.tag-name.django",
				"settings": {
					"foreground": "#fe6948"
				}
			},
			{
				"scope": "support.function.magic.python",
				"settings": {
					"foreground": "#f5aa64"
				}
			}
		]
	},
	"python.analysis.autoImportCompletions": true,
	"files.associations": {
		"**/views/**/*.html": "django-html",
		"*.tpl": "django-html"
	},
	"[django-html]": {
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "junstyle.vscode-django-support"
	}
}
